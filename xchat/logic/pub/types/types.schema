struct ChatMessage {
	ChatID uint64
	ID  uint64
	User   string
	Ts     int64
	Msg    string
	Updated int64
	IsRoomChat bool
}

struct ChatNotifyMessage {
	ChatID uint64
	User   string
	Ts     int64
	Msg    string
	Updated int64
	IsRoomChat bool
}

struct XMessage {
	Msg union {
		ChatMessage
		ChatNotifyMessage
	}
}
