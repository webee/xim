struct ChatMessage {
	ChatID uint64
	ChatType string
	ID  uint64
	User   string
	Ts     int64
	Msg    string
	Updated int64
}

struct ChatNotifyMessage {
	ChatID uint64
	ChatType string
	User   string
	Ts     int64
	Msg    string
	Updated int64
}

struct SetAreaLimitCmd {
	Limit uint32
}

struct XMessage {
	Msg union {
		ChatMessage
		ChatNotifyMessage
		SetAreaLimitCmd
	}
}
