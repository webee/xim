struct MsgSource {
	InstanceID uint64
	SessionID uint64
}

struct UserNotifyMessage {
	User   string
	Msg    string
}

struct ChatMessage {
	ChatID uint64
	ChatType string
	ID  uint64
	User   string
	Ts     int64
	Msg    string
	Updated int64
}

struct ChatNotifyMessage {
	ChatID uint64
	ChatType string
	User   string
	Ts     int64
	Msg    string
	Updated int64
}

struct SetAreaLimitCmd {
	Limit uint32
}

struct XMessage {
	Source *MsgSource
	Msg union {
		UserNotifyMessage
		ChatMessage
		ChatNotifyMessage
		SetAreaLimitCmd
	}
}
